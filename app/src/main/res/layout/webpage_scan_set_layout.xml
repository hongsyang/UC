<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/grey"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <TextView
        android:id="@+id/flip_way"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="   翻页方式"
        android:textColor="#000000"
        android:gravity="center_vertical"
        android:drawableRight="@drawable/triangle"
        android:background="@drawable/whitebackground"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/grey"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <Switch
        android:id="@+id/start_open_last_page"
        android:text="   启动时打开上次未关闭网页"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#fff"/>
    <View
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <Switch
        android:id="@+id/left_right_slide"
        android:text="   左右滑屏前进后退"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#fff"/>
    <View
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <Switch
        android:id="@+id/two_fingers_slide"
        android:text="   双指滑动管理窗口"
        android:textColor="#000000"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#fff"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/grey"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/whitebackground">
        <TextView
            android:id="@+id/search_find"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="   搜索发现"/>
        <TextView
            android:id="@+id/search_show_module"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="   搜索时展现搜索发现模块"
            android:textSize="10sp"/>
        <Switch
            android:id="@+id/search_find_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp" />

    </RelativeLayout>
    <View
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/whitebackground">
        <TextView
            android:id="@+id/everyone_search"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="   大家都在搜"/>
        <TextView
            android:id="@+id/show_everyone_search"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="   浏览网页时，展示智能推荐'大家都在搜'"
            android:textSize="10sp"/>
        <Switch
            android:id="@+id/everyone_search_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp" />

    </RelativeLayout>
    <View
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/whitebackground">
        <TextView
            android:id="@+id/related_recommend"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="   功能提醒通知"/>
        <TextView
            android:id="@+id/show_relate_recommend"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="   浏览网页时，智能推荐相关新闻"
            android:textSize="10sp"/>
        <Switch
            android:id="@+id/related_recommend_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@drawable/grey"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/whitebackground">
        <TextView
            android:id="@+id/subscribe_remind"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="   订阅提醒"/>
        <TextView
            android:id="@+id/show_subscribe_remind"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="   展示搜索结果页相关内容更新的订阅提示"
            android:textSize="10sp"/>
        <Switch
            android:id="@+id/subscribe_remind_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="10dp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#CCCCCC" />
    <View
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@drawable/grey"/>

</LinearLayout>
